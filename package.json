{
  "name": "@kunlunjs/fabric",
  "version": "0.10.2",
  "description": "ESLint, Prettier, StyleLint, husky, lint-staged, git commit message verify configuration for front-end projects",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    ".npmrc",
    ".husky",
    ".vscode",
    "dist",
    "generate",
    "src",
    ".editorconfig",
    ".eslintignore",
    ".prettierignore",
    ".stylelintignore",
    "client.d.ts",
    "lint-staged.config.js",
    "verify-commit-msg.js",
    "tsconfig.browser.json",
    "tsconfig.server.json"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "tsc -p tsconfig.build.json",
    "postinstall": "node dist/pregenerator.js",
    "lint": "eslint --ext .js,.jsx,.cjs,.mjs,.ts,.tsx ./src --config .eslintrc.js --ignore-path .eslintignore --fix --cache",
    "stylelint": "stylelint \"**/*.{css,scss,sass,less}\" \"!**/min.css/**\" --config ./stylelint.config.js --allow-empty-input -i ./.stylelintignore --fix -f verbose",
    "prettier": "prettier \"**/*.+(js|json|ts|tsx)\" --config prettier.config.js --ignore-unknown --ignore-path ./.prettierignore --list-different --cache",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --check --list-different",
    "types:check": "tsc --project tsconfig.json --pretty --noEmit",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kunlunjs/kunlun-fabric.git"
  },
  "keywords": [
    "git",
    "husky",
    "vscode",
    "eslint",
    "prettier",
    "stylelint",
    "lint-staged"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kunlunjs/kunlun-fabric/issues"
  },
  "homepage": "https://github.com/kunlunjs/kunlun-fabric#readme",
  "dependencies": {
    "@babel/core": "^7.22.20",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@rushstack/eslint-patch": "^1.4.0",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "command-exists": "^1.2.9",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-define-config": "^1.23.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-mdx": "^2.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-let": "^3.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-regexp": "^1.15.0",
    "eslint-plugin-testing-library": "^6.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "node-emoji": "^2.1.0",
    "ora": "5.4.1",
    "postcss": "^8.4.30",
    "postcss-scss": "^4.0.8",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "stylelint": "^15.10.3",
    "stylelint-config-css-modules": "^4.3.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "stylelint-no-unsupported-browser-features": "^7.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/command-exists": "^1.2.1",
    "@types/eslint": "^8.44.2",
    "@types/node": "^20.6.2",
    "devmoji": "^2.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "release-it": "^16.1.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@types/eslint": "*",
    "@types/node": "*",
    "devmoji": "*",
    "eslint": "*",
    "husky": "*",
    "lint-staged": "*",
    "prettier": "*",
    "stylelint": "*",
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": false
    },
    "@types/eslint": {
      "optional": true
    },
    "husky": {
      "optional": false
    },
    "lint-staged": {
      "optional": false
    },
    "devmoji": {
      "optional": false
    },
    "eslint": {
      "optional": false
    },
    "prettier": {
      "optional": false
    },
    "stylelint": {
      "optional": true
    },
    "typescript": {
      "optional": false
    }
  }
}
