{
  "name": "@kunlunjs/fabric",
  "version": "0.1.0",
  "description": "ESLint/Prettier/StyleLint configuration for Kunlun projects",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    ".husky",
    "generate",
    "src",
    "dist",
    ".editorconfig",
    ".eslintignore",
    ".prettierignore",
    ".stylelintignore",
    "lint-staged.config.js",
    "verify-commit-msg.js"
  ],
  "engines": {
    "node": ">=16.14.0"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "tsc -p tsconfig.build.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "node dist/generator.js",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/turing-fe/kunlun-fabric.git"
  },
  "keywords": [
    "git",
    "husky",
    "vscode",
    "eslint",
    "prettier",
    "stylelint",
    "lint-staged"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/turing-fe/kunlun-fabric/issues"
  },
  "homepage": "https://github.com/turing-fe/kunlun-fabric#readme",
  "dependencies": {
    "@rushstack/eslint-patch": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "chalk": "4.1.2",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.4.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-tailwindcss": "^3.5.0",
    "eslint-plugin-testing-library": "^5.3.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fs-extra": "^10.1.0",
    "node-emoji": "^1.11.0",
    "ora": "5.4.1",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.10",
    "stylelint": "^14.8.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-no-unsupported-browser-features": "^5.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/eslint": "^8.4.1",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.29",
    "@types/node-emoji": "^1.8.1",
    "@types/prettier": "^2.6.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "release-it": "^14.14.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "eslint": ">=8.10.0",
    "prettier": ">=2.6.0"
  },
  "peerDependenciesMeta": {
    "@types/eslint": {
      "optional": true
    },
    "@types/node": {
      "optional": true
    },
    "@types/prettier": {
      "optional": true
    },
    "husky": {
      "optional": false
    },
    "stylelint": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  }
}
