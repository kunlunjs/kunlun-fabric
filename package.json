{
  "name": "@kunlunjs/fabric",
  "version": "0.6.3",
  "description": "ESLint, Prettier, StyleLint, husky, lint-staged, git commit message verify configuration for front-end projects",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    ".husky",
    ".vscode",
    "dist",
    "generate",
    "src",
    ".editorconfig",
    ".eslintignore",
    ".prettierignore",
    ".stylelintignore",
    "client.d.ts",
    "lint-staged.config.js",
    "verify-commit-msg.js",
    "tsconfig.browser.json",
    "tsconfig.server.json"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "tsc -p tsconfig.build.json",
    "postinstall": "node dist/pregenerator.js",
    "types:check": "tsc --pretty --noEmit",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kunlunjs/kunlun-fabric.git"
  },
  "keywords": [
    "git",
    "husky",
    "vscode",
    "eslint",
    "prettier",
    "stylelint",
    "lint-staged"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kunlunjs/kunlun-fabric/issues"
  },
  "homepage": "https://github.com/kunlunjs/kunlun-fabric#readme",
  "dependencies": {
    "@babel/core": "^7.21.4",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/preset-react": "^7.18.6",
    "@rushstack/eslint-patch": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "chalk": "4.1.2",
    "command-exists": "^1.2.9",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-define-config": "^1.17.0",
    "eslint-import-resolver-node": "^0.3.7",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-mdx": "^2.0.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-let": "^3.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-regexp": "^1.14.0",
    "eslint-plugin-testing-library": "^5.10.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fs-extra": "^11.1.1",
    "node-emoji": "^1.11.0",
    "ora": "5.4.1",
    "postcss": "^8.4.21",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "stylelint": "^15.4.0",
    "stylelint-config-css-modules": "^4.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard": "^32.0.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "stylelint-no-unsupported-browser-features": "^6.1.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/command-exists": "^1.2.0",
    "@types/eslint": "^8.37.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.15.11",
    "@types/node-emoji": "^1.8.2",
    "@types/prettier": "^2.7.2",
    "devmoji": "^2.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "release-it": "^15.10.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
