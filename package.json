{
  "name": "@kunlunjs/fabric",
  "version": "0.2.17",
  "description": "ESLint/Prettier/StyleLint/Husky/lint-staged/git commit message verify configuration for front-end projects",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    ".husky",
    ".vscode",
    "dist",
    "generate",
    "src",
    ".editorconfig",
    ".eslintignore",
    ".prettierignore",
    ".stylelintignore",
    "lint-staged.config.js",
    "verify-commit-msg.js"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "tsc -p tsconfig.build.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "node dist/pregenerator.js",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kunlunjs/kunlun-fabric.git"
  },
  "keywords": [
    "git",
    "husky",
    "vscode",
    "eslint",
    "prettier",
    "stylelint",
    "lint-staged"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kunlunjs/kunlun-fabric/issues"
  },
  "homepage": "https://github.com/kunlunjs/kunlun-fabric#readme",
  "dependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "chalk": "4.1.2",
    "command-exists": "^1.2.9",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.6.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.7",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.6.0",
    "eslint-plugin-testing-library": "^5.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fs-extra": "^10.1.0",
    "node-emoji": "^1.11.0",
    "ora": "5.4.1",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "stylelint": "^14.11.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-no-unsupported-browser-features": "^5.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.0",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/command-exists": "^1.2.0",
    "@types/eslint": "^8.4.6",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.7.13",
    "@types/node-emoji": "^1.8.1",
    "@types/prettier": "^2.7.0",
    "devmoji": "^2.3.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "release-it": "^15.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "peerDependencies": {
    "@types/eslint": "latest",
    "@types/node": "latest",
    "@types/prettier": "latest",
    "eslint": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "prettier": "latest",
    "stylelint": "latest"
  },
  "peerDependenciesMeta": {
    "@types/eslint": {
      "optional": true
    },
    "@types/node": {
      "optional": true
    },
    "@types/prettier": {
      "optional": true
    },
    "eslint": {
      "optional": false
    },
    "husky": {
      "optional": true
    },
    "lint-staged": {
      "optional": true
    },
    "prettier": {
      "optional": false
    },
    "stylelint": {
      "optional": true
    }
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/eslint",
        "@types/node",
        "@types/prettier",
        "husky",
        "lint-staged",
        "stylelint"
      ],
      "allowedVersions": {
        "eslint": "8",
        "husky": "8",
        "lint-staged": "12",
        "prettier": "2",
        "stylelint": "14"
      }
    }
  }
}
